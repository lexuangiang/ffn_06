<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for [:admin, @rate], html: {multipart: true} do |f| %>
      <%= render "shared/error_messages", object: @rate %>

      <%= f.label :rate_name, class: "col-md-4 control-label" %>
      <%= f.text_field :rate_name, class: "form-control" %>

      <%= f.label :home_rate, class: "col-md-4 control-label" %>
      <%= f.text_field :home_rate, class: "form-control" %>

      <%= f.label :away_rate, class: "col-md-4 control-label" %>
      <%= f.text_field :away_rate, class: "form-control" %>

      <%= f.label :category_match_rate_id, class: "col-md-4 control-label" %>
      <%= f.collection_select :category_match_rate_id, @category_match_rates,
        :id, :category_rates, prompt: true %>

      <%= f.submit %>
    <% end %>
  </div>
</div>
